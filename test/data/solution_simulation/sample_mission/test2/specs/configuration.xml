<?xml version="1.0" encoding="UTF-8"?>
<qxcfg>
  <TransportNetwork>
    <intervals-nooverlap>false</intervals-nooverlap>
    <use-transfer-location>false</use-transfer-location>
    <search>
      <interactive>false</interactive>
      <options>
        <adaptive_computation_distance>40</adaptive_computation_distance>
        <computation_distance>120</computation_distance>
        <cutoff>2</cutoff>
        <epoch_timeout_in_s>60</epoch_timeout_in_s>
        <hill-climbing>false</hill-climbing>
        <lpsolver>GLPK</lpsolver>
        <master-slave>false</master-slave>
        <nogoods_limit>128</nogoods_limit>
        <threads>1</threads>
        <total_timeout_in_s>900</total_timeout_in_s>
        <cost-function>
          <efficacy>
            <weight>1.0</weight>
          </efficacy>
          <efficiency>
            <weight>1.0</weight>
          </efficiency>
          <safety>
            <weight>1.0</weight>
          </safety>
        </cost-function>
        <model-usage>
          <afc-decay>0.95</afc-decay>
        </model-usage>
        <role-usage>
          <afc-decay>0.95</afc-decay>
          <force-min>true</force-min>
          <immobile>
            <bound-offset>0</bound-offset>
            <bounded>true</bounded>
          </immobile>
          <mobile>
            <bound-offset>0</bound-offset>
            <bounded>true</bounded>
          </mobile>
        </role-usage>
        <timeline-brancher>
          <afc-decay>0.95</afc-decay>
          <supply-demand>false</supply-demand>
        </timeline-brancher>
      </options>
    </search>
  </TransportNetwork>
</qxcfg>
